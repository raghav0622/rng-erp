{
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit",
    "source.organizeImports": "explicit"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "hide-files.files": [
    ".next",
    "node_modules",
    "tsconfig.tsbuildinfo",
    ".storybook",
    ".vscode",
    "public",
    ".env.example",
    ".env.local",
    ".prettierrc",
    "next-env.d.ts",
    "package-lock.json",
    "vitest.shims.d.ts",
    "vitest.config.ts",
    "postcss.config.cjs",
    "next.config.ts",
    "types",
    "app/robots.ts",
    "app/loading.tsx",
    "app/page.tsx",
    "app/layout.tsx",
    "app/globals.css"
  ],
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    ".gitignore": true,
    ".next": true,
    "node_modules": true,
    "tsconfig.tsbuildinfo": true,
    ".storybook": true,
    ".vscode": true,
    "public": true,
    ".env.example": true,
    ".env.local": true,
    ".prettierrc": true,
    "next-env.d.ts": true,
    "package-lock.json": true,
    "vitest.shims.d.ts": true,
    "vitest.config.ts": true,
    "postcss.config.cjs": true,
    "next.config.ts": true,
    "types": true,
    "app/robots.ts": true,
    "app/loading.tsx": true,
    "app/page.tsx": true,
    "app/layout.tsx": true,
    "app/globals.css": true
  }
}

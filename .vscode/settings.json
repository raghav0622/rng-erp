{
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit",
    "source.organizeImports": "explicit"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "node_modules": true,
    "storybook-static": true,
    ".next": true,
    ".github/workflows": true,
    ".github": true,
    ".vscode": true,
    "next-env.d.ts": true,
    "package-lock.json": true,
    "postcss.config.cjs": true,
    "tsconfig.json": true,
    "tsconfig.tsbuildinfo": true,
    "vitest.config.ts": true,
    "vitest.shims.d.ts": true,
    "eslint.config.mjs": true,
    ".storybook": true,
    ".prettierrc": true,
    ".gitignore": true,
    ".env.local": true,
    "next.config.ts": true
  },
  "hide-files.files": [
    "node_modules",
    "storybook-static",
    ".next",
    ".github/workflows",
    ".github",
    ".vscode",
    "next-env.d.ts",
    "package-lock.json",
    "postcss.config.cjs",
    "tsconfig.json",
    "tsconfig.tsbuildinfo",
    "vitest.config.ts",
    "vitest.shims.d.ts",
    "eslint.config.mjs",
    ".storybook",
    ".prettierrc",
    ".gitignore",
    ".env.local",
    "next.config.ts"
  ]
}

{
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit",
    "source.organizeImports": "explicit"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "hide-files.files": [
    ".github",
    ".storybook",
    ".vscode",
    "node_modules",
    ".env.local",
    ".prettierrc",
    "eslint.config.mjs",
    "next-env.d.ts",
    "package-lock.json",
    "postcss.config.cjs",
    "tsconfig.json",
    "tsconfig.tsbuildinfo",
    "vitest.config.ts",
    "vitest.shims.d.ts",
    ".next/cache/swc/plugins/windows_x86_64_23.0.0",
    ".next/cache/swc/plugins",
    ".next"
  ],
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    ".gitignore": true,
    ".github": true,
    ".storybook": true,
    ".vscode": true,
    "node_modules": true,
    ".env.local": true,
    ".prettierrc": true,
    "eslint.config.mjs": true,
    "next-env.d.ts": true,
    "package-lock.json": true,
    "postcss.config.cjs": true,
    "tsconfig.json": true,
    "tsconfig.tsbuildinfo": true,
    "vitest.config.ts": true,
    "vitest.shims.d.ts": true,
    ".next/cache/swc/plugins/windows_x86_64_23.0.0": true,
    ".next/cache/swc/plugins": true,
    ".next": true
  }
}
